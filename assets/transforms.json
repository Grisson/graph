{
  "directory" : [{
    "name" : "Expanded",
    "script" : "templateTransform.js",
    "data" : {
      "settings" : {
        "clustered": false,
        "showEdgeLabels": false,
        "showNodeLabels": true
      },
      "nodeMaps" : [{
        "properties" : {
          "type" : "$.type", 
          "name" : "$.label",
          "tags" : "$.metadata",
          "metadata" : "$.metadata"
        }
      }],
      "edgeMaps" : [{
        "properties" : {
          "type" : "$.relation",
          "label" : "$.relation",
          "tags" : "$.metadata",
          "source" : "$.source",
          "target" : "$.target",
          "metadata" : "$.metadata"
        }
      },{
        "filter" : {
          "eval" : "('%s' == 'loadBalances')",
          "args" : [ "$.relation" ]
        }, 
        "properties" : {
          "type" : "balances",
          "label" : "balances"
        }
      },{
        "filter" : {
          "eval" : "('%s' == 'createdFrom')",
          "args" : [ "$.relation" ]
        }, 
        "properties" : {
          "type" : "uses",
          "label" : "uses"
        }
      }]
    }
  },{
    "name" : "Clustered",
    "script" : "templateTransform.js",
    "data" : {
      "settings" : {
        "clustered": true,
        "showEdgeLabels": false,
        "showNodeLabels": true
      },
      "nodeMaps" : [{
        "properties" : {
          "type" : "$.type", 
          "name" : "$.label",
          "tags" : "$.metadata",
          "metadata" : "$.metadata"
        }
      }],
      "edgeMaps" : [{
        "properties" : {
          "type" : "$.relation",
          "label" : "$.relation",
          "tags" : "$.metadata",
          "source" : "$.source",
          "target" : "$.target",
          "metadata" : "$.metadata"
        }
      },{
        "filter" : {
          "eval" : "('%s' == 'loadBalances')",
          "args" : [ "$.relation" ]
        }, 
        "properties" : {
          "type" : "balances",
          "label" : "balances"
        }
      },{
        "filter" : {
          "eval" : "('%s' == 'createdFrom')",
          "args" : [ "$.relation" ]
        }, 
        "properties" : {
          "type" : "uses",
          "label" : "uses"
        }
      }]
    }
  }]
}