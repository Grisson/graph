{
  "directory" : [{
    "name" : "All Data",
    "script" : "templateTransform.js",
    "data" : {
      "nodeMaps" : [{
        "filter" : {
          "expr" : "true",
          "args" : []
        }, 
        "properties" : {
          "type" : "type", 
          "name" : "$.label"
        }
      }],
      "edgeMaps" : [{
        "filter" : {
          "expr" : "true",
          "args" : []
        }, 
        "properties" : {
          "type" : "$.relation",
          "label" : "$.relation"
        }
      }]
    }
  },{
    "name" : "Pods",
    "script" : "templateTransform.js",
    "data" : {
      "nodeMaps" : [{
        "filter" : {
          "expr" : "('%s' == 'Container')",
          "args" : [ "$.type" ]
        }, 
        "properties" : {
          "type" : "type", 
          "name" : "$.metadata.Config.Image", 
          "tags" : {
            "Hostname" : "$.metadata.Config.Hostname", 
            "Memory" : "$.metadata.Config.Memory", 
            "MemorySwap" : "$.metadata.Config.MemorySwap", 
            "NetworkDisabled" : "$.metadata.Config.NetworkDisabled", 
            "Dns" : "$.metadata.HostConfig.Dns", 
            "DnsSearch" : "$.metadata.HostConfig.DnsSearch"
          }
        }
      },{
        "filter" : {
          "expr" : "('%s' == 'Pod')",
          "args" : [ "$.type" ]
        }, 
        "properties" : {
          "type" : "type", 
          "name" : "$.label",
          "tags" : {
            "containerID" : "$.metadata.currentState.info.POD.containerID", 
            "image" : "$.metadata.currentState.info.POD.image", 
            "podIP" : "$.metadata.currentState.info.POD.podIP", 
            "startedAt" : "$.metadata.currentState.info.POD.state.running.startedAt"
          }
        }
      }],
      "nodeFilters" : [{
        "expr" : "('%s' == 'Pod') || ('%s' == 'Container')",
        "args" : [ "$.type", "$.type" ]
      }],
      "edgeMaps" : [{
        "filter" : {
          "expr" : "('%s' == 'contains')",
          "args" : [ "$.relation" ]
        }, 
        "properties" : {
          "type" : "$.relation",
          "label" : "$.relation"
        }
      }],
      "edgeFilters" : [{
        "expr" : "('%s' === 'contains')",
        "args" : [ "$.relation" ]
      }]
    }
  },{
    "name" : "Services",
    "script" : "templateTransform.js",
    "data" : {
      "nodeMaps" : [{
        "filter" : {
          "expr" : "('%s' == 'Project')",
          "args" : [ "$.type" ]
        }, 
        "properties" : {
          "type" : "type", 
          "name" : "$.label",
          "tags" : {
      	    "ID": "$.id",
      	    "Label": "$.label",
      	    "internalID": "$.metadata.internalID",
      	    "name" : "$.metadata.name"
          }
        }
      },{
        "filter" : {
          "expr" : "('%s' == 'Service')",
          "args" : [ "$.type" ]
        }, 
        "properties" : {
          "type" : "type", 
          "name" : "$.label",
          "tags" : {
      	    "ID": "$.id",
      	    "Label": "$.label",
      	    "internalID": "$.metadata.internalID",
      	    "name" : "$.metadata.name"
          }
        }
      }],
      "nodeFilters" : [{
        "expr" : "('%s' == 'Project') || ('%s' == 'Service')",
        "args" : [ "$.type", "$.type" ]
      }],
      "edgeMaps" : [{
        "filter" : {
          "expr" : "('%s' == 'contains')",
          "args" : [ "$.relation" ]
        }, 
        "properties" : {
          "type" : "$.relation",
          "label" : "$.relation"
        }
      }],
      "edgeFilters" : [{
        "expr" : "('%s' === 'contains')",
        "args" : [ "$.relation" ]
      }]
    }
  }]
}